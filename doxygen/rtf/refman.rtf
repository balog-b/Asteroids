390 {\cf20 /* free memory - called via the macro.}\par
391 {\cf20  * as all allocations are tracked in the list, this function can terminate the program}\par
392 {\cf20  * if a block is freed twice or the free function is called with an invalid address. */}\par
393 {\cf17 static} {\cf18 void} debugmalloc_free_full({\cf18 void} *mem, {\cf18 char} {\cf17 const} *func, {\cf18 char} {\cf17 const} *file, {\cf18 unsigned} line) \{\par
394     {\cf20 /* imitate standard free function: if ptr is null, no operation is performed */}\par
395     {\cf19 if} (mem == NULL)\par
396         {\cf19 return};\par
397 \par
398     {\cf20 /* find allocation, abort if not found */}\par
399     DebugmallocEntry *deleted = debugmalloc_find(mem);\par
400     {\cf19 if} (deleted == NULL) \{\par
401         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: olyan teruletet probalsz felszabaditani, ami nincs lefoglalva!\\n"}, func, file, line);\par
402         abort();\par
403     \}\par
404 \par
405     {\cf20 /* check canary and then free memory */}\par
406     {\cf19 if} (!debugmalloc_canary_ok(deleted)) \{\par
407         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: a %p memoriateruletet tulindexelted!\\n"}, func, file, line, mem);\par
408         debugmalloc_dump_elem(deleted);\par
409     \}\par
