318 {\cf20 /* find element in hash table, given with the memory address that the user sees.}\par
319 {\cf20  * @return the linked list entry, or null if not found. */}\par
320 {\cf17 static} DebugmallocEntry *debugmalloc_find({\cf18 void} *mem) \{\par
321     DebugmallocData *instance = debugmalloc_singleton();\par
322     {\cf18 size_t} idx = debugmalloc_hash(mem);\par
323     DebugmallocEntry *head = &instance->head[idx];\par
324     {\cf19 for} (DebugmallocEntry *iter = head->next; iter->next != NULL; iter = iter->next)\par
325         {\cf19 if} (iter->user_mem == mem)\par
326             {\cf19 return} iter;\par
327     {\cf19 return} NULL;\par
328 \}\par
