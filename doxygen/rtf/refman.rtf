331 {\cf20 /* allocate memory. this function is called via the macro. */}\par
332 {\cf17 static} {\cf18 void} *debugmalloc_malloc_full({\cf18 size_t} size, {\cf18 char} {\cf17 const} *func, {\cf18 char} {\cf17 const} *expr, {\cf18 char} {\cf17 const} *file, {\cf18 unsigned} line, {\cf18 bool} zero) \{\par
333     {\cf20 /* imitate standard malloc: return null if size is zero */}\par
334     {\cf19 if} (size == 0)\par
335         {\cf19 return} NULL;\par
336     \par
337     {\cf20 /* check max size */}\par
338     DebugmallocData *instance = debugmalloc_singleton();\par
339     {\cf19 if} (size > instance->max_block_size) \{\par
340         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: a blokk merete tul nagy, %u bajt; debugmalloc_max_block_size() fuggvennyel novelheto.\\n"}, func, file, line, ({\cf18 unsigned}) size);\par
341         abort();\par
342     \}\par
343 \par
344     {\cf20 /* allocate more memory, make room for canary */}\par
345     {\cf18 void} *real_mem = malloc(size + 2 * debugmalloc_canary_size);\par
346     {\cf19 if} (real_mem == NULL) \{\par
347         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: nem sikerult %u meretu memoriat foglalni!\\n"}, func, file, line, ({\cf18 unsigned}) size);\par
348         {\cf19 return} NULL;\par
349     \}\par
350 \par
351     {\cf20 /* allocate memory for linked list element */}\par
352     DebugmallocEntry *newentry = (DebugmallocEntry *) malloc({\cf17 sizeof}(DebugmallocEntry));\par
353     {\cf19 if} (newentry == NULL) \{\par
354         free(real_mem);\par
355         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: le tudtam foglalni %u memoriat, de utana a sajatnak nem, sry\\n"}, func, file, line, ({\cf18 unsigned}) size);\par
356         abort();\par
357     \}\par
358 \par
359     {\cf20 /* metadata of allocation: caller function, code line etc. */}\par
360     debugmalloc_strlcpy(newentry->func, func, {\cf17 sizeof}(newentry->func));\par
361     debugmalloc_strlcpy(newentry->expr, expr, {\cf17 sizeof}(newentry->expr));\par
362     debugmalloc_strlcpy(newentry->file, file, {\cf17 sizeof}(newentry->file));\par
363     newentry->line = line;\par
364 \par
365     {\cf20 /* address of allocated memory chunk */}\par
366     newentry->real_mem = real_mem;\par
367     newentry->user_mem = ({\cf18 unsigned} {\cf18 char} *) real_mem + debugmalloc_canary_size;\par
368     newentry->size = size;\par
369     debugmalloc_memory_init(newentry, zero);\par
370 \par
371     {\cf20 /* store in list and return pointer to user area */}\par
372     debugmalloc_insert(newentry);\par
373     {\cf19 return} newentry->user_mem;\par
374 \}\par
