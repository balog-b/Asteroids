414 {\cf20 /* realloc-like function. */}\par
415 {\cf17 static} {\cf18 void} *debugmalloc_realloc_full({\cf18 void} *oldmem, {\cf18 size_t} newsize, {\cf18 char} {\cf17 const} *func, {\cf18 char} {\cf17 const} *expr, {\cf18 char} {\cf17 const} *file, {\cf18 unsigned} line) \{\par
416     {\cf20 /* imitate standard realloc: equivalent to free if size is null. */}\par
417     {\cf19 if} (newsize == 0) \{\par
418         debugmalloc_free_full(oldmem, func, file, line);\par
419         {\cf19 return} NULL;\par
420     \}\par
421     {\cf20 /* imitate standard realloc: equivalent to malloc if first param is NULL */}\par
422     {\cf19 if} (oldmem == NULL)\par
423         {\cf19 return} debugmalloc_malloc_full(newsize, func, expr, file, line, 0);\par
424 \par
425     {\cf20 /* find old allocation. abort if not found. */}\par
426     DebugmallocEntry *oldentry = debugmalloc_find(oldmem);\par
427     {\cf19 if} (oldentry == NULL) \{\par
428         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: olyan teruletet probalsz atmeretezni, ami nincs lefoglalva!\\n"}, func, file, line);\par
429         abort();\par
430     \}\par
431 \par
432     {\cf20 /* create new allocation, copy & free old data */}\par
433     {\cf18 void} *newmem = debugmalloc_malloc_full(newsize, func, expr, file, line, {\cf17 false});\par
434     {\cf19 if} (newmem == NULL) \{\par
435         debugmalloc_log({\cf22 "debugmalloc: %s @ %s:%u: nem sikerult uj memoriat foglalni az atmeretezeshez!\\n"}, func, file, line);\par
436         {\cf20 /* imitate standard realloc: original block is untouched, but return NULL */}\par
437         {\cf19 return} NULL;\par
438     \}\par
439     {\cf18 size_t} smaller = oldentry->size < newsize ? oldentry->size : newsize;\par
440     memcpy(newmem, oldmem, smaller);\par
441     debugmalloc_free_inner(oldentry);\par
442 \par
443     {\cf19 return} newmem;\par
444 \}\par
