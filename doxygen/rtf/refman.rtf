283 {\cf20 /* hash function for bucket hash. */}\par
284 {\cf17 static} {\cf18 size_t} debugmalloc_hash({\cf18 void} *address) \{\par
285     {\cf20 /* the last few bits are ignored, as they are usually zero for}\par
286 {\cf20      * alignment purposes. all tested architectures used 16 byte allocation. */}\par
287     {\cf18 size_t} cut = (size_t)address >> 4;\par
288     {\cf19 return} cut % debugmalloc_tablesize;\par
289 \}\par
