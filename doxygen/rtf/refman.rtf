377 {\cf20 /* free memory and remove list item. before deleting, the chuck is filled with}\par
378 {\cf20  * the canary byte to make sure that the user will see garbage if the memory}\par
379 {\cf20  * is accessed after freeing. */}\par
380 {\cf17 static} {\cf18 void} debugmalloc_free_inner(DebugmallocEntry *deleted) \{\par
381     debugmalloc_remove(deleted);\par
382 \par
383     {\cf20 /* fill with garbage, then remove from linked list */}\par
384     memset(deleted->real_mem, debugmalloc_canary_char, deleted->size + 2 * debugmalloc_canary_size);\par
385     free(deleted->real_mem);\par
386     free(deleted);\par
387 \}\par
