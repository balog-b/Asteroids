292 {\cf20 /* insert element to hash table. */}\par
293 {\cf17 static} {\cf18 void} debugmalloc_insert(DebugmallocEntry *entry) \{\par
294     DebugmallocData *instance = debugmalloc_singleton();\par
295     {\cf18 size_t} idx = debugmalloc_hash(entry->user_mem);\par
296     DebugmallocEntry *head = &instance->head[idx];\par
297     entry->prev = head;\par
298     entry->next = head->next;\par
299     head->next->prev = entry;\par
300     head->next = entry;\par
301     instance->alloc_count += 1;\par
302     instance->alloc_bytes += entry->size;\par
303     instance->all_alloc_count += 1;\par
304     instance->all_alloc_bytes += entry->size;\par
305 \}\par
