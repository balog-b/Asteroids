308 {\cf20 /* remove element from hash table */}\par
309 {\cf17 static} {\cf18 void} debugmalloc_remove(DebugmallocEntry *entry) \{\par
310     DebugmallocData *instance = debugmalloc_singleton();\par
311     entry->next->prev = entry->prev;\par
312     entry->prev->next = entry->next;\par
313     instance->alloc_count -= 1;\par
314     instance->alloc_bytes -= entry->size;\par
315 \}\par
